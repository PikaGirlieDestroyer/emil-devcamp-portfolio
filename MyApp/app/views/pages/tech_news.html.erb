<div class="inner cover">
  <h1 class="cover-heading news-header">Rails News</h1>
  
  <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tech-news news-cards">
          <div class="card news-cards">
            <%= twitter_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>


